{
	"name": "tice-kralt-portfolio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"tsc": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"tsc:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format:fix": "prettier --write .",
		"format:check": "prettier --check .",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"lint": "npm run format:check && npm run lint:check && npm run tsc"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
